
<h1>Novi kolegij</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for(@page) do |f| %>
		<%= f.label :ime %>
		<%= f.text_field :name, class: 'form-control' %>
		<%= f.label :opis %>
		<%= f.text_field :description, class: 'form-control' %>
		<%= f.submit "Stvori novi kolegij", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>

